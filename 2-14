public class Pair<T, U> {
    private T first;
    private U second;

    public Pair(T first, U second) {
        this.first = first;
        this.second = second;
    }

    public T getFirst() {
        return first;
    }

    public U getSecond() {
        return second;
    }

    public static void main(String[] args) {
        Pair<Integer, String> intStringPair = new Pair<>(42, "Hello, World!");
        Pair<String, Double> stringDoublePair = new Pair<>("foo", 3.14);

        System.out.println("First: " + intStringPair.getFirst());
        System.out.println("Second: " + intStringPair.getSecond());

        System.out.println("First: " + stringDoublePair.getFirst());
        System.out.println("Second: " + stringDoublePair.getSecond());
    }
}
